"remap.rb '\Cv'"
   Control + v

"remap.rb '\Cu'"
   Control + u

"remap.rb '\Ci'"
   Control + i

"remap.rb '\Cw'"
   Control + w

"remap.rb '\C/'"
   Control + slash

"remap.rb '\Ct'"
   Control + t

"remap.rb '\Cp'"
   Control + p

"remap.rb '\Cg'"
   Control + g

"xvkbd -xsendevent -text '\[Down]' > /dev/null 2> /dev/null"
   Control + j

"xvkbd -xsendevent -text '\[Up]' > /dev/null 2> /dev/null"
   Control + k

"xvkbd -xsendevent -text '\[Right]' > /dev/null 2> /dev/null"
   Control + d

"xvkbd -xsendevent -text '\[Left]' > /dev/null 2> /dev/null"
   Control + semicolon

"remap.rb '\Cm'"
   Control + m

"remap.rb '\Al'"
   Alt + l

"remap.rb '\An'"
   Alt + n 

"remap.rb '\At'"
   Alt + t

"remap.rb '\Aw'"
   Alt + w

"remap.rb '\Ag'"
   Alt + g

"remap.rb '\Au'"
   Alt + u

"remap.rb '\Az'"
   Alt + z

"remap.rb '\Ax'"
   Alt + x

"remap.rb '\Ac'"
   Alt + c

"remap.rb '\Av'"
   Alt + v

"remap.rb '\Ad'"
   Alt + d

"remap.rb '\AN'"
  Alt + Shift + N
